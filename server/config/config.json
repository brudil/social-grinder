{
	"accounts": {
		"personal-twitter": {
			"module": "twitter",
			"cache": 160,
			"settings": {
				"username": "brudil",
				"include-retweets": true

			}
		},

		"sharmech-twitter": {
			"module": "twitter",
			"cache": 160,
			"settings": {
				"username": "sharmech",
				"include-retweets": true

			}
		},

		"whs-twitter": {
			"module": "twitter",
			"cache": 300,
			"settings": {
				"username": "woollyhat",
				"include-retweets": true
			}
		},

		"personal-github": {
			"module": "github",
			"cache": 60,
			"settings": {
				"username": "brudil"
			}
		},

		"sharmech-github": {
			"module": "github",
			"cache": 60,
			"settings": {
				"username": "sharmech"
			}
		},

		"whs-blog": {
			"module": "rss",
			"cache": 200,
			"settings": {
				"url": "http://woollyhatstudios.com/feed/"
			}
		}

	},

	"streams": {
		"personal": {
			"accounts":[
				"personal-twitter",
				"whs-twitter",
				"personal-github"
			],
			"cors": true,
			"count": 15

		},
		"sharmech": {
			"accounts":[
				"sharmech-twitter",
				"whs-twitter",
				"sharmech-github"
			],
			"cors": true,
			"count": 7

		}

	},

	"default-stream": "personal",
	"enable-cors": true
}